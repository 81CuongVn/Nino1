{
	"sourceMaps": true,
	"inlineSourcesContent": true,
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"target": "es2016"
	},

	"module": {
		"type": "commonjs"
	}
}
